<% layout('layouts/boilerplate') %>
<main class="partnerships">
  <h2>Partnerships</h2>
  <div class="members">
    <% partnershipMembers.forEach((member)=>{ %>
    <div class="memberdiv">
      <h3><% member.title %></h3>
      <a href="<%= member.website %>" target="_blank"
        ><img src="<%= member.image %>" alt="<%= member.title %>"
      /></a>
      <p><%= member.text %></p>
    </div>
    <% }) %>
  </div>
</main>
<script src="/js/partnerships.js"></script>
