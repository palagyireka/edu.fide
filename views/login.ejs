<main>
  <h1>Sign in</h1>
  <p><%= error %></p>
  <form action="/login" method="post">
    <div>
      <label for="username">Username</label>
      <input type="text" name="username" id="username" placeholder="username" />
    </div>
    <div>
      <label for="password">Password</label>
      <input
        type="password"
        name="password"
        id="password"
        placeholder="password"
      />
    </div>
    <button>Sign In</button>
  </form>
</main>
