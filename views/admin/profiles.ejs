<% layout('layouts/boilerplate') %>

<div class="profiles-container">
  <% userProfiles.forEach((profile) => { %>
  <div class="admin-page-profile">
    <div class="profile-email profile-data">
      <div>Email</div>
      <div><%= profile.email %></div>
    </div>
    <div class="profile-name profile-data">
      <div>Name</div>
      <div><%= profile.firstName %> <%= profile.lastName %></div>
    </div>
    <div class="profile-workplace profile-data">
      <div>Workplace</div>
      <div><% profile.workplace %></div>
    </div>
    <div class="profile-jobtitle profile-data">
      <div>Jobtitle</div>
      <div><%= profile.jobtitle %></div>
    </div>
    <div class="profile-responsibilities profile-data">
      <div>Responsibilities</div>
      <div><%= profile.respCie %></div>
    </div>
    <div class="profile-registration-date profile-data">
      <div>Registration Date</div>
      <div><%= profile.registrationDate.toDateString() %></div>
    </div>
    <div class="profile-newsletter profile-data">
      <div>Subscribed to newsletter?</div>
      <div><%= profile.newsletter === true ? "Yes" : "No" %></div>
    </div>
  </div>
  <% }) %>
</div>
